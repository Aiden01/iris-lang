fn getInput() {
    return int(input("Enter a number: "));
}
val n = 10;
val usersGuess = getInput();
while (usersGuess != n) {
    if (usersGuess < n) {
        print("Higher!");
    }
    if (usersGuess > n) {
        print("Lower!");
    }
    usersGuess = getInput();
}
print("Nice, you got it right! The number was " ++ str(n));
print(randElement([1, 2, 3]));